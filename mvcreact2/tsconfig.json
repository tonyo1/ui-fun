{
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "commonjs",
        "noImplicitAny": true,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": true,
        "target": "es6",
        "jsx": "react-jsx",
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "outDir": "./js/_tsOBJ"
    },
    "exclude": [
        "node_modules",
        "wwwroot"
    ]
}